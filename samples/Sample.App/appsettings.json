{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServerTest": {
    "UserName": "admin",
    "Password": "password123",
    "VirtualHost": "/",
    "Hosts": [ "rabbitmq" ],
    "ClientProvidedName": "Sample.App"
  },
  "ProducerTest": {
    "ChannelPool": {
      "MinPoolSize": 2,
      "MaxPoolSize": 4,
      "WaitTimeoutMs": 1000
    }
  },
  "ConsumerTestSimple": {
    "ConsumersCount": 5,
    "Queue": {
      "UseMessageTypeAsQueueName": true
    },
    "Retry": {
      "PerQueueTtlMs": 5000,
      "PerMessageTtlOnRetryMs": [ "5000", "5000", "5000" ]
    }
  },
  "TopicConsumerUsaOrders": {
    "ConsumersCount": 2,
    "Queue": {
      "Name": "topic.orders.usa"
    },
    "Exchanges": [
      {
        "Name": "exchange.topic",
        "Type": "topic",
        "DeclareExchange": true,
        "RoutingKeys": [ "orders.usa.*" ]
      }
    ],
    "Retry": {
      "PerQueueTtlMs": 2000,
      "PerMessageTtlOnRetryMs": [ "2000", "2000", "2000" ]
    }
  },
  "TopicConsumerEuropeOrders": {
    "ConsumersCount": 2,
    "Queue": {
      "Name": "topic.orders.europe"
    },
    "Exchanges": [
      {
        "Name": "exchange.topic",
        "Type": "topic",
        "DeclareExchange": true,
        "RoutingKeys": [ "orders.europe.*" ]
      }
    ],
    "Retry": {
      "PerQueueTtlMs": 2000,
      "PerMessageTtlOnRetryMs": [ "2000", "2000", "2000" ]
    }
  },
  "TopicConsumerLogs": {
    "ConsumersCount": 2,
    "Queue": {
      "Name": "topic.logs"
    },
    "Exchanges": [
      {
        "Name": "exchange.topic",
        "Type": "topic",
        "DeclareExchange": true,
        "RoutingKeys": [ "logs.#" ]
      }
    ],
    "Retry": {
      "PerQueueTtlMs": 2000,
      "PerMessageTtlOnRetryMs": [ "2000", "2000", "2000" ]
    }
  },
  "FanoutConsumer": {
    "ConsumersCount": 2,
    "Queue": {
      "Name": "FanoutQueue"
    },
    "Exchanges": [
      {
        "Name": "testapp.other.fanout",
        "Type": "fanout",
        "DeclareExchange": true
      }
    ],
    "Retry": {
      "PerQueueTtlMs": 2000,
      "PerMessageTtlOnRetryMs": [ "2000", "2000", "2000" ]
    }
  },
  "RetryExampleConsumer": {
    "ConsumersCount": 1,
    "Queue": {
      "Name": "retry.example"
    },
    "Retry": {
      "PerMessageTtlOnRetryMs": [ "5000", "15000", "60000" ]
    }
  }
}
